<div id="tl_soverview">
    <div id="tl_main_action">
        <h2>{{ 'member_import.import_information'|trans([], 'contao_member_import') }}</h2>
        <strong>{{ 'member_import.selected'|trans([], 'contao_member_import') }}</strong>
        <ul>
            {%- for setting in settings ~%}
            <li>{{ setting }}</li>
            {%- endfor ~%}
        </ul>
    </div>
    <div id="tl_load_action" class="not_active">
        <h2>{{ 'member_import.import_load'|trans([], 'contao_member_import') }}</h2>
        <strong>{{ 'member_import.progress'|trans([], 'contao_member_import') }}</strong>
        <div class="progress-bar stripes animated reverse slower">
            <div class="progress-bar-inner"></div>
        </div>
    </div>
    <div id="tl_prepare_action" class="not_active">
        <h2>{{ 'member_import.import_prepare'|trans([], 'contao_member_import') }}</h2>
        <strong>{{ 'member_import.progress'|trans([], 'contao_member_import') }}</strong>
        <div class="progress-bar stripes animated reverse slower">
            <div class="progress-bar-inner"></div>
        </div>
    </div>
    <div id="tl_import_action" class="not_active">
        <h2>{{ 'member_import.import_import'|trans([], 'contao_member_import') }}</h2>
        <strong>{{ 'member_import.progress'|trans([], 'contao_member_import') }}</strong>
        <div class="progress-bar stripes animated reverse slower">
            <div class="progress-bar-inner"></div>
        </div>
    </div>
    <div id="error"></div>
</div>
<div class="tl_formbody_submit">
    <div class="tl_submit_container">
        <button type="submit" name="terminate" id="terminate" class="tl_submit" accesskey="t">{{ 'MSC.cancelBT'|trans([], 'contao_default') }}</button>
    </div>
</div>
