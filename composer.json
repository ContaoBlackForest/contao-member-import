{
    "name": "contaoblackforest/contao-member-import",
    "description": "Contao member import for Contao OpenSource CMS",
    "keywords": [
        "contao",
        "backend",
        "member",
        "import",
        "bundle"
    ],
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Sven Baumann",
            "email": "baumann.sv@gmail.com",
            "homepage": "https://github.com/ContaoBlackforest",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "baumann.sv@gmail.com",
        "issues": "https://github.com/ContaoBlackforest/contao-member-import/issues",
        "wiki": "https://github.com/ContaoBlackforest/contao-member-import/wiki",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/ContaoBlackforest/contao-member-import"
    },
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.4",
        "contao-community-alliance/translator": "^2.1",
        "contao-community-alliance/contao-polyfill-bundle": "^1.0",
        "menatwork/contao-multicolumnwizard-bundle": "^3.4",
        "symfony/dependency-injection": "^3.4 || ^4.3",
        "symfony/event-dispatcher": "^3.4 || ^4.3",
        "symfony/http-foundation": "^3.4 || ^4.3",
        "ext-pdo": "*"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "ContaoBlackForest\\Member\\Import\\": "legacy",
            "ContaoBlackForest\\MemberImportBundle\\": "src"
        }
    },
    "extra": {
        "contao-manager-plugin": "ContaoBlackForest\\MemberImportBundle\\ContaoManager\\Plugin",
        "branch-alias": {
            "dev-release/2.0.0": "2.0.x-dev"
        }
    }
}
